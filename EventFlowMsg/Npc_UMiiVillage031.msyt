      - text: 50束木柴束
      - control:
          kind: reset_colour
      - text: 過來了嗎？
      - control:
          kind: choice
          choice_labels:
            - 12
            - 13
          selected_index: 0
          cancel_index: 1
          unknown: 6
  talk42:
    attributes: Npc_UMiiVillage031
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 請盡快將
      - control:
          kind: set_colour
