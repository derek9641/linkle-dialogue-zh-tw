  NPC_Umahonephoto001_R_003:
    attributes: Npc_Hunters_Lost
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "你要為人家的獨家頭條\n好好奮鬥哦！\n因為名叫"
