  talk45:
    attributes: Npc_UMiiVillage031
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 是真的。
      - control:
          kind: single_choice
          label: 17
  talk46:
    attributes: Npc_UMiiVillage031
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
