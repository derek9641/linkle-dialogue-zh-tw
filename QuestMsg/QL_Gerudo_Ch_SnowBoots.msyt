group_count: 2
atr1_unknown: 0
entries:
  QL_Gerudo_Ch_SnowBoots_Desc:
    contents:
      - text: 第八位英雄其實曾經持有
      - control:
          kind: set_colour
          colour: red
      - text: 寶劍
      - control:
          kind: reset_colour
      - text: "！\n\n雖然可能是伯騰薩亂說地，\n但還是值得找找看。\n據說第八位英雄所持有的"
      - control:
          kind: set_colour
          colour: red
      - text: 寶劍
      - control:
          kind: reset_colour
      - text: "，\n就沉睡在格魯德高地的某處。\n給伯騰薩看看第八位英雄的寶劍的相片，\n獲得"
      - control:
          kind: set_colour
          colour: red
      - text: 雪地靴
      - control:
          kind: reset_colour
      - text: 吧！
  QL_Gerudo_Ch_SnowBoots_Finish:
    contents:
      - text: "你在格魯德高地的山頂\n找到了第八位英雄的寶劍。\n\n拍下這把劍的照片，\n伯騰薩也依約將雪地靴送給你了。"
  QL_Gerudo_Ch_SnowBoots_Name:
    contents:
      - text: 被遺忘的劍
