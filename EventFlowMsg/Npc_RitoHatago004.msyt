          unknown: 10
  Talk06:
    attributes: Npc_RitoHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: 請好好休息！
  Talk07:
    attributes: Npc_RitoHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: 那麼，請好好休息！！
  Talk08:
    attributes: Npc_RitoHatago004
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 11
      - text: 早安，有睡飽嗎？
      - control:
          kind: pause
          length: longer
      - text: "\n路上請小心。"
  Talk09:
    attributes: Npc_RitoHatago004
