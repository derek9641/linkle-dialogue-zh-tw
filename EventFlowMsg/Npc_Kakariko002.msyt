          unknown: 10
  Talk01:
    attributes: Npc_Kakariko002
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 如果有什麼我能幫上忙的地方，
      - control:
          kind: pause
          length: short
      - text: "\n請儘管開口。"
      - control:
          kind: choice
          choice_labels:
            - 1
            - 2
            - 3
