    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 那就快點吃哩～
  talk14:
    attributes: Npc_SouthernVillage004
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: 很好吃吧！
      - control:
          kind: pause
          length: short
