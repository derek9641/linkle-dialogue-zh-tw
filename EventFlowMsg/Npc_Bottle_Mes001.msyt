  talk09:
    attributes: Npc_Bottle_Mes001
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: 啊……
      - control:
          kind: pause
          length: short
      - text: "是你啊。\n\n在那之後我總算是抵達了"
      - control:
          kind: set_colour
          colour: blue
