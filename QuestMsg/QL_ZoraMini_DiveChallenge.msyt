group_count: 3
atr1_unknown: 0
entries:
  QL_ZoraMini_DiveChallenge_Desc:
    contents:
      - text: "諾爾聳恿你從這裡\n跳到下面的水池。\n\n不是腳朝下，\n而是得"
      - control:
          kind: set_colour
          colour: red
      - text: 頭朝下
      - control:
          kind: reset_colour
      - text: "跳下去，\n不然就不算數。"
  QL_ZoraMini_DiveChallenge_Finish:
    contents:
      - text: "你成功完成了跳水和攀瀑。\n諾爾的靈魂好像受到了極大的衝擊。"
  QL_ZoraMini_DiveChallenge_Name:
    contents:
      - text: 陶醉在跳水中的男人
  QL_ZoraMini_DiveChallenge_Step1:
    contents:
      - text: "你頭朝下地跳進下面的水池，\n得到了諾爾的認可。\n\n接著要挑戰"
      - control:
          kind: set_colour
          colour: red
      - text: 攀瀑
      - control:
          kind: reset_colour
      - text: 。
  QL_ZoraMini_DiveChallenge_Step2:
    contents:
      - text: "你從下面的水池逆流而上，\n成功地攀上瀑布。\n\n去聽聽"
      - control:
          kind: set_colour
          colour: red
      - text: 諾爾
      - control:
          kind: reset_colour
      - text: 的感想吧。
