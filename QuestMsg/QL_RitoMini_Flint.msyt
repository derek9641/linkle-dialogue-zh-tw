group_count: 2
atr1_unknown: 0
entries:
  QL_RitoMini_Flint_Desc:
    contents:
      - text: "來利特村度蜜月的久戈\n好像想為妻子準備烤蘋果。\n\n要製作烤蘋果，自然需要用火。\n\n拿"
      - control:
          kind: set_colour
          colour: red
      - text: 打火石
      - control:
          kind: reset_colour
      - text: 給他生火吧。
  QL_RitoMini_Flint_Finish:
    contents:
      - text: "你將打火石交給了\n來利特村度蜜月的久戈，得到了盧比當做謝禮！\n\n久戈似乎還想\n多做點烤蘋果。\n\n獲得"
      - control:
          kind: set_colour
          colour: red
      - text: 打火石
      - control:
          kind: reset_colour
      - text: "之後，\n再一起拿給他吧！"
  QL_RitoMini_Flint_Name:
    contents:
      - text: 新郎換回名譽
