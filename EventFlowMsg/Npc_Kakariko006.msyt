entries:
  "0000":
    attributes: ""
    contents:
      - text: 買
  "0001":
    attributes: ""
    contents:
      - text: 再見
  "0002":
    attributes: ""
    contents:
      - text: 請教我！
  "0003":
    attributes: ""
    contents:
      - text: 不用了
  "0004":
    attributes: ""
    contents:
      - text: 防禦的精髓？
  "0005":
    attributes: ""
    contents:
      - text: 回避跳躍
  "0006":
    attributes: ""
    contents:
      - text: 完美防禦
  "0007":
    attributes: ""
    contents:
      - text: 我好像都看到了……
  "0008":
    attributes: ""
    contents:
      - text: 我什麼都沒看到……
  "0009":
    attributes: ""
    contents:
      - text: 橫向跳
  "0010":
    attributes: ""
    contents:
      - text: 後空翻
  "0013":
    attributes: ""
    contents:
      - text: 料理？
  Near00:
    attributes: Npc_Kakariko006
    contents:
      - text: 需要南瓜嗎？
  Near01:
    attributes: Npc_Kakariko006
    contents:
      - text: 才不會輸給雨呢。
  Near02:
    attributes: Npc_Kakariko006
    contents:
      - text: 呵呵呵。
  Near03:
    attributes: Npc_Kakariko006
    contents:
      - text: 可惡！可惡！
  Near04:
    attributes: Npc_Kakariko006
    contents:
      - text: 再栽培吧。
  Near05:
    attributes: Npc_Kakariko006
    contents:
      - text: 今天天氣也很好喏。
  Near06:
    attributes: Npc_Kakariko006
    contents:
      - text: 天降甘霖喏。
  Near07:
    attributes: Npc_Kakariko006
    contents:
      - text: 肚子餓喏。
  Near08:
    attributes: Npc_Kakariko006
    contents:
      - text: 疲勞都消除了喏。
  Near09:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: 南瓜最棒了……
  Near10:
    attributes: Npc_Kakariko006
    contents:
      - text: 該回家了喏。
  Near11:
    attributes: Npc_Kakariko006
    contents:
      - text: 今天也好累喏。
  SupriseHouse:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 1
      - control:
          kind: sound2
          unknown:
            - 19
            - 205
      - text: 哇、哇喔！？
  SupriseHouse_00:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 18
      - text: "搞什麼……是你啊……\n\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 除了老伴以外，沒人會待在這個家裡嘛。
      - control:
          kind: pause
          length: longer
      - text: "\n別這樣嚇人啦。"
  SupriseHouse_01:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 1
      - text: …
      - control:
          kind: pause
          length: short
      - text: …
      - control:
          kind: pause
          length: longer
  Talk00:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: 很中意
      - control:
          kind: set_colour
          colour: blue
      - text: 鎧甲南瓜
      - control:
          kind: reset_colour
      - text: 嗎？
  Talk01:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "呵呵呵，\n你選那顆，眼光真是出乎意料的好喏。"
  Talk02:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: 呵呵呵，
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - control:
          kind: reset_colour
      - text: "\n全賣光喏。\n努力栽培的南瓜能幫上別人，\n真令老夫高興喏。\n下次得努力栽培出\n更優質的南瓜才行喏。"
  Talk03:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 13
      - text: 唔……
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - control:
          kind: reset_colour
      - text: "\n這明明好吃到極點喏……"
  Talk04:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: 呵呵呵，
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - control:
          kind: reset_colour
      - text: "\n想要"
      - control:
          kind: set_colour
          colour: blue
      - text: 鎧甲南瓜
      - control:
          kind: reset_colour
      - text: "喏？\n老夫現在正要過去田裡喏，"
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - control:
          kind: reset_colour
      - text: "\n你再等一下喏。"
  Talk05:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 25
      - text: 抱歉喏，老夫要去吃晚餐喏。
      - control:
          kind: pause
          length: longer
      - text: "\n想買南瓜的話麻煩明天再來喏。"
  Talk06:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - control:
          kind: variable
          variable_kind: 11
          name: PlacedItemName
      - control:
          kind: reset_colour
      - text: 是
      - control:
          kind: set_colour
          colour: red
      - control:
          kind: variable
          variable_kind: 16
          name: PlacedItemPrice
      - text: 盧比
      - control:
          kind: reset_colour
      - text: 。
      - control:
          kind: pause
          length: longer
      - text: "\n買1顆回去如何喏？"
      - control:
          kind: choice
          choice_labels:
            - 0
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk07:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 7
            - 1
      - control:
          kind: sound2
          unknown:
            - 21
            - 205
      - text: "還是老伴煮的\n"
      - control:
          kind: set_colour
          colour: blue
      - text: 鎧甲南瓜
      - control:
          kind: reset_colour
      - text: "料理最好吃了喏。\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "比胡蘿蔔什麼的\n好吃太多了喏。"
  Talk08:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: 抱歉打斷你的興致喏。
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - control:
          kind: reset_colour
      - text: "\n\n但老伴很寶貝這片梅花園喏。"
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - control:
          kind: reset_colour
      - text: "\n還請你好好愛惜。"
  Talk09:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 1
      - control:
          kind: sound2
          unknown:
            - 17
            - 205
      - text: ……誰在那裡？
  Talk10:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 這麼晚跑來有什麼事喏……
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - control:
          kind: reset_colour
      - text: "\n\n鍛鍊腰腿可是防禦的入門喏。\n所以老夫才會像這樣在夜晚跑步。\n要讓身體學會「防禦的精髓」，\n鍛鍊是最佳做法喏。"
      - control:
          kind: choice
          choice_labels:
            - 4
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk11:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 9
      - text: 呵呵呵……希卡族是有著優異智慧的民族。
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - control:
          kind: reset_colour
      - text: "\n光憑武力是保護不了心愛事物的喏。\n雖然戰鬥的方法因人而異，\n但這部分還是得好好牢記喔。"
      - control:
          kind: choice
          choice_labels:
            - 4
            - 5
            - 6
            - 3
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Talk12:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "村裡的神廟\n供奉著從前人稱劍聖的高手。\n據說只要通過祂們的考驗，\n就能學會防禦的精髓。\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 0
      - text: "沒人知道那個「考驗」\n究竟是什麼……\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "不過為了能隨時接受「考驗」，\n時時刻刻都不能怠忽鍛鍊呢。"
  Talk13:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 雖然有個暴力老頭一直嚷嚷
      - control:
          kind: pause
          length: short
      - text: "\n攻擊才是最大的防禦。\n但老夫認為防禦磨練至極致之人\n才是最強的。"
  Talk14:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 在注視並掌握敵人行動的狀態下往旁邊跳躍，
      - control:
          kind: pause
          length: longer
      - text: "\n老夫等人將其稱為橫向跳。\n雖說只是防禦，\n但也要防禦得很帥氣才行喏。"
  Talk16:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 23
      - text: "呵呵呵……村裡難得\n能見到像你這樣的年輕人。"
  Talk18:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: sound
          unknown:
            - 0
            - 11
      - text: 鎧甲南瓜
      - control:
          kind: reset_colour
      - text: "能強健體魄。\n老伴都會做成料理，\n"
      - control:
          kind: raw
          four:
            three:
              field_1: 0
      - text: 讓老夫天天吃呢。
      - control:
          kind: choice
          choice_labels:
            - 13
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk19:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: 呵呵呵，老夫說笑的。
      - control:
          kind: pause
          length: long
      - text: "\n誰叫你一臉正經喏。"
      - control:
          kind: pause
          length: longer
  Talk20:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 10
            - 1
      - control:
          kind: sound2
          unknown:
            - 20
            - 205
      - text: "是、是是是你啊……\n有……有什麼事喏……"
      - control:
          kind: choice
          choice_labels:
            - 7
            - 8
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Talk21:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: 這是卡卡利科村的特產
      - control:
          kind: set_colour
          colour: blue
      - text: 鎧甲南瓜
      - control:
          kind: reset_colour
      - text: 。
      - control:
          kind: pause
          length: longer
      - text: "\n是老夫精心栽培的喏～非常好吃喔。\n還想要的話，老夫可以分你喏。"
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - control:
          kind: reset_colour
      - text: "\n等老夫在時，來田裡一趟就行。"
  Talk22:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "今天已經很晚了。\n差不多該回家了喏。"
  Talk23:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 9
      - text: "呵呵呵，\n那個暴力老頭還是老樣子……\n什麼「攻擊才是最大的防禦！」"
      - control:
          kind: pause
          length: longer
      - text: "\n\n又在嚷嚷這種鬼話喏。\n還真是辛苦他了喏……"
  Talk24:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 32
      - text: "老夫年輕時總是在村中到處衝刺，\n大家都稱老夫是不會喘的納金……\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "但不管怎麼鍛鍊，\n都敵不過年紀啊。"
  Talk25:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: "如果想將其他技巧磨練到極致，\n可別客氣，來問老夫吧。"
      - control:
          kind: choice
          choice_labels:
            - 4
            - 5
            - 6
            - 3
          selected_index: 0
          cancel_index: 3
          unknown: 10
  Talk26:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 在下雨天晚上跑來有什麼事喏……
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - control:
          kind: reset_colour
      - text: "\n\n鍛鍊腰腿可是防禦的入門喏。\n所以老夫才會像這樣在夜晚跑步。\n要讓身體學會「防禦的精髓」，\n鍛鍊是最佳做法喏。"
      - control:
          kind: choice
          choice_labels:
            - 4
            - 1
          selected_index: 0
          cancel_index: 1
          unknown: 6
  Talk27:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 9
            - 13
      - text: 被、被你看到了嗎……
      - control:
          kind: pause
          length: longer
      - text: "\n\n老夫跟歌思汀\n從以前就合不來喏……"
      - control:
          kind: pause
          length: long
      - text: "\n一起去打獵時，老夫都是先蹲下，\n再悄悄地靠近，但那傢伙卻會一邊嚷嚷……\n"
      - control:
          kind: sound
          unknown:
            - 8
            - 7
      - text: "「攻擊是最佳做法！」\n\n"
      - control:
          kind: sound
          unknown:
            - 9
            - 1
      - text: "然後立刻衝出去。\n每次都害獵物逃走喏……\n此外，還堅持不讓"
      - control:
          kind: set_colour
          colour: blue
      - text: 材料店
      - control:
          kind: reset_colour
      - text: "賣南瓜……\n\n"
      - control:
          kind: sound2
          unknown:
            - 10
            - 205
      - text: 老夫到底做錯了什麼……
      - control:
          kind: pause
          length: longer
      - text: "\n實在好不甘心喏……"
  Talk28:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 1
            - 18
      - text: "呵呵呵，那就好。\n\n……不過老夫現在有點忙。\n\n有事的話，\n麻煩晚點再來喏。"
  Talk29:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 8
            - 22
      - text: 你……
      - control:
          kind: pause
          length: long
      - text: "該不會是……\n"
      - control:
          kind: set_colour
          colour: blue
      - text: 依蓋隊
      - control:
          kind: reset_colour
      - text: 喏…
      - control:
          kind: pause
          length: short
      - text: …
      - control:
          kind: pause
          length: longer
  Talk30:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 24
      - text: 不過被你看到後，老夫清醒了。
      - control:
          kind: pause
          length: longer
      - text: "\n\n破壞田地洩憤的行為，\n就到今天為止喏……"
  Talk31:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 跳躍是回避的基本功喏。
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - control:
          kind: reset_colour
      - text: "\n希望你能牢記這兩項喏。"
      - control:
          kind: choice
          choice_labels:
            - 9
            - 10
          selected_index: 0
          cancel_index: 2
          unknown: 6
  Talk32:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 在注視並掌握敵人行動的狀態下往後跳躍，
      - control:
          kind: set_colour
          colour: blue
      - control:
          kind: pause
          length: short
      - control:
          kind: reset_colour
      - text: "\n老夫等人將其稱為後空翻。\n"
      - control:
          kind: sound2
          unknown:
            - 7
            - 205
      - text: "呵呵呵，如何？就連老夫這樣的老頭\n也很懂新潮用詞對吧。"
  Talk34:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 發誓效忠
      - control:
          kind: set_colour
          colour: red
      - text: 災厄加儂
      - control:
          kind: reset_colour
      - text: "的人，\n被我們稱為"
      - control:
          kind: set_colour
          colour: blue
      - text: 依蓋隊
      - control:
          kind: reset_colour
      - text: "。\n"
      - control:
          kind: sound
          unknown:
            - 11
            - 1
      - control:
          kind: sound2
          unknown:
            - 15
            - 205
      - text: 他們對與
      - control:
          kind: set_colour
          colour: red
      - text: 加儂
      - control:
          kind: reset_colour
      - text: 敵對之人毫不留情……
      - control:
          kind: pause
          length: longer
      - text: "\n前陣子的大地震後，他們的行動似乎更頻繁了喏。"
      - control:
          kind: pause
          length: longer
      - text: "\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 但老夫並不認為你是
      - control:
          kind: set_colour
          colour: blue
      - text: 依蓋隊
      - control:
          kind: reset_colour
      - text: 啦。
      - control:
          kind: pause
          length: short
      - text: "\n這點小事，老夫還是分得出來的喏……"
      - control:
          kind: pause
          length: longer
  Talk35:
    attributes: Npc_Kakariko006
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 使用
      - control:
          kind: set_colour
          colour: blue
      - text: 鎧甲南瓜
      - control:
          kind: reset_colour
