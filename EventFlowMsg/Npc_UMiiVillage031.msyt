          unknown: 6
  talk59:
    attributes: Npc_UMiiVillage031
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
      - text: 知道了，
      - control:
          kind: pause
          length: short
      - text: "\n做好心理準備再跟我說一聲吧。"
  talk60:
    attributes: Npc_UMiiVillage031
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 0
