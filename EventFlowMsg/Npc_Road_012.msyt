  MSG_Gerud_00:
    attributes: Npc_Road_012
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 2
      - text: 尋寶資歷20年……
      - control:
          kind: pause
          length: long
      - text: "\n傳說中的寶物獵人，就是我！\n"
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "話說，現在鎮上似乎一直在談論\n名叫「"
      - control:
          kind: set_colour
          colour: red
      - text: 依蓋隊
      - control:
          kind: reset_colour
      - text: "」的盜賊……\n雖然寶物獵人和盜賊大不相同，"
      - control:
          kind: pause
          length: short
      - text: "\n但某種意義上，他們或許也算我的勁敵吧！"
  MSG_Gerud_00_1:
    attributes: Npc_Road_012
    contents:
      - control:
          kind: sound
          unknown:
            - 0
            - 4
      - text: "鎮上似乎一直在談論\n名叫「"
      - control:
          kind: set_colour
